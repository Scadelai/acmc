void main (void){
    int size;
    int temp;
    int final;
    int i;
    int one;

    one = 1;
    i = 0;
    final = 0;

    size = input();
    output(size);
    while (i < size){
        temp = input();
        output(temp);
        final = final + temp;
        i = i + one;
    }

    output(final);
}